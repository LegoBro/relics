execute at @e[type=armor_stand,tag=board.player.1,tag=get_id] run setblock ~ ~-2 ~ chest
execute at @e[type=armor_stand,tag=board.player.1,tag=get_id] run setblock ~ ~-3 ~ shulker_box

execute at @e[type=armor_stand,tag=board.player.2,tag=get_id] run setblock ~ ~-2 ~ chest
execute at @e[type=armor_stand,tag=board.player.2,tag=get_id] run setblock ~ ~-3 ~ shulker_box

scoreboard players set #friendly var 1
execute as @e[type=armor_stand,tag=board.player.1,tag=get_id] at @s run function cards:entity/hero/set
execute at @e[type=armor_stand,tag=board.player.1,tag=get_id] run tag @e[tag=card.hero,tag=new,tag=id,limit=1,sort=nearest,distance=..2] add hero.1
scoreboard players set #friendly var 0
execute as @e[type=armor_stand,tag=board.player.2,tag=get_id] at @s run function cards:entity/hero/set
execute at @e[type=armor_stand,tag=board.player.2,tag=get_id] run tag @e[tag=card.hero,tag=new,tag=id,limit=1,sort=nearest,distance=..2] add hero.2

execute as @e[tag=board,tag=get_id] run function main:board/cell/display
scoreboard players operation @e[tag=get_id] id = game.id var
tag @e[tag=get_id] add id
tag @e[tag=get_id] remove get_id
